{
  "name": "client",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --mode=development",
    "dev:staging": "vite --mode=staging",
    "dev:prod": "vite --mode=production",
    "build": "tsc -b && vite build",
    "build:staging": "tsc -b && vite build --mode=staging",
    "build:prod": "tsc -b && vite build --mode=production",
    "preview": "vite preview",
    "favicons": "bun scripts/generate-favicons.ts",
    "deploy:pages": "wrangler pages deploy dist --env=preview",
    "deploy:pages:prod": "bun run build:prod && wrangler pages deploy dist --project-name=canthus-org",
    "deploy:pages:staging": "bun run build:staging && wrangler pages deploy dist --env=staging",
    "pages:dev": "wrangler pages dev dist",
    "setup:env": "./scripts/setup-env.sh",
    "validate:env": "node -e \"console.log('Environment validation will run on app startup')\""
  },
